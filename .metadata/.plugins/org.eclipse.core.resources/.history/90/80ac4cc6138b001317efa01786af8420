
public class Rectangle {
	private int length;
	private int width;
	

	public Rectangle (int l, int w) {
		length = l;
		width = w;
	}
	
	public void setLength (int l) {
		length = l;
	}

	public void setWidth (int w) {
		width = w;
	}
	
	public int getLength (){
		return length;
	}
	
	public int getWidth (){
		return length;
	}
	
	public int getArea (){
		return length * width;
	}

	public int getPerimeter (){
		return (length * 2) + (width * 2);
	}
}
